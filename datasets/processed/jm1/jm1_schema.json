{
  "title": "JM1",
  "description": "JM1/software defect prediction\r\n",
  "modelCategory": "binary_classification",
  "schemaVersion": 1.0,
  "inputDataFormat": "CSV",
  "encoding": "utf-8",
  "id": {
    "name": "id",
    "description": "unique identifier"
  },
  "target": {
    "name": "defects",
    "description": "binary class (true or false)",
    "classes": [
      "False",
      "True"
    ]
  },
  "features": [
    {
      "name": "loc",
      "description": "loc : numeric % McCabe's line count of code",
      "dataType": "NUMERIC",
      "example": 1.1,
      "nullable": false
    },
    {
      "name": "v(g)",
      "description": "v(g) : numeric % McCabe \"cyclomatic complexity\"",
      "dataType": "NUMERIC",
      "example": 1.4,
      "nullable": false
    },
    {
      "name": "ev(g)",
      "description": "ev(g) : numeric % McCabe \"essential complexity\"",
      "dataType": "NUMERIC",
      "example": 1.4,
      "nullable": false
    },
    {
      "name": "iv(g)",
      "description": "iv(g) : numeric % McCabe \"design complexity\"",
      "dataType": "NUMERIC",
      "example": 1.4,
      "nullable": false
    },
    {
      "name": "n",
      "description": "n : numeric % Halstead total operators + operands",
      "dataType": "NUMERIC",
      "example": 1.3,
      "nullable": false
    },
    {
      "name": "v",
      "description": "v : numeric % Halstead \"volume\"",
      "dataType": "NUMERIC",
      "example": 1.3,
      "nullable": false
    },
    {
      "name": "l",
      "description": "l : numeric % Halstead \"program length\"",
      "dataType": "NUMERIC",
      "example": 1.3,
      "nullable": false
    },
    {
      "name": "d",
      "description": "d : numeric % Halstead \"difficulty\"",
      "dataType": "NUMERIC",
      "example": 1.3,
      "nullable": false
    },
    {
      "name": "i",
      "description": "i : numeric % Halstead \"intelligence\"",
      "dataType": "NUMERIC",
      "example": 1.3,
      "nullable": false
    },
    {
      "name": "e",
      "description": "e : numeric % Halstead \"effort\"",
      "dataType": "NUMERIC",
      "example": 1.3,
      "nullable": false
    },
    {
      "name": "b",
      "description": "b : numeric % Halstead",
      "dataType": "NUMERIC",
      "example": 1.3,
      "nullable": false
    },
    {
      "name": "t",
      "description": "t : numeric % Halstead's time estimator",
      "dataType": "NUMERIC",
      "example": 1.3,
      "nullable": false
    },
    {
      "name": "lOCode",
      "description": "lOCode : numeric % Halstead's line count",
      "dataType": "NUMERIC",
      "example": 2.0,
      "nullable": false
    },
    {
      "name": "lOComment",
      "description": "lOComment : numeric % Halstead's count of lines of comments",
      "dataType": "NUMERIC",
      "example": 2.0,
      "nullable": false
    },
    {
      "name": "lOBlank",
      "description": "lOBlank : numeric % Halstead's count of blank lines",
      "dataType": "NUMERIC",
      "example": 2.0,
      "nullable": false
    },
    {
      "name": "locCodeAndComment",
      "description": "lOCodeAndComment: numeric",
      "dataType": "NUMERIC",
      "example": 2.0,
      "nullable": false
    },
    {
      "name": "uniq_Op",
      "description": "uniq_Op : numeric % unique operators",
      "dataType": "NUMERIC",
      "example": 1.2,
      "nullable": true
    },
    {
      "name": "uniq_Opnd",
      "description": "uniq_Opnd : numeric % unique operands",
      "dataType": "NUMERIC",
      "example": 1.2,
      "nullable": true
    },
    {
      "name": "total_Op",
      "description": "total_Op : numeric % total operators",
      "dataType": "NUMERIC",
      "example": 1.2,
      "nullable": true
    },
    {
      "name": "total_Opnd",
      "description": "total_Opnd : numeric % total operands 21: branchCount : numeric % of the flow graph",
      "dataType": "NUMERIC",
      "example": 1.2,
      "nullable": true
    },
    {
      "name": "branchCount",
      "description": "branchCount : numeric % of the flow graph",
      "dataType": "NUMERIC",
      "example": 1.4,
      "nullable": true
    }
  ]
}