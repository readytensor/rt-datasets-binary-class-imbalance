{
  "title": "Concentric Spheres Dataset",
  "description": "Synthetically generated concentric spheres dataset.",
  "modelCategory": "binary_classification",
  "schemaVersion": 1.0,
  "inputDataFormat": "CSV",
  "encoding": "utf-8",
  "id": {
    "name": "id",
    "description": "unique identifier"
  },
  "target": {
    "name": "target",
    "description": "binary class (0 or 1)",
    "classes": [
      "0",
      "1"
    ]
  },
  "features": [
    {
      "name": "x",
      "description": "x coordinate",
      "dataType": "NUMERIC",
      "example": -0.1528,
      "nullable": true
    },
    {
      "name": "y",
      "description": "y coordinate",
      "dataType": "NUMERIC",
      "example": -0.4802,
      "nullable": true
    },
    {
      "name": "z",
      "description": "z coordinate",
      "dataType": "NUMERIC",
      "example": 0.0808,
      "nullable": true
    },
    {
      "name": "dummy_1",
      "description": "dummy feature 1",
      "dataType": "NUMERIC",
      "example": 0.5872,
      "nullable": true
    },
    {
      "name": "dummy_2",
      "description": "dummy feature 2",
      "dataType": "NUMERIC",
      "example": 0.8254,
      "nullable": true
    },
    {
      "name": "dummy_3",
      "description": "dummy feature 3",
      "dataType": "NUMERIC",
      "example": 0.2315,
      "nullable": true
    },
    {
      "name": "corr_x",
      "description": "feature correlated with x coordinate",
      "dataType": "NUMERIC",
      "example": 0.141,
      "nullable": true
    },
    {
      "name": "corr_y",
      "description": "feature correlated with y coordinate",
      "dataType": "NUMERIC",
      "example": -0.1877,
      "nullable": true
    },
    {
      "name": "corr_z",
      "description": "feature correlated with z coordinate",
      "dataType": "NUMERIC",
      "example": -0.0508,
      "nullable": true
    }
  ]
}