{
  "title": "Wilt",
  "description": "High-resolution Remote Sensing data set (Quickbird). Small number of training samples of diseased trees, large number for other land cover. Testing data set from stratified random sample of image.",
  "modelCategory": "binary_classification",
  "schemaVersion": 1.0,
  "inputDataFormat": "CSV",
  "encoding": "utf-8",
  "id": {
    "name": "id",
    "description": "unique identifier"
  },
  "target": {
    "name": "class",
    "description": "binary class (1 or 2)",
    "classes": [
      "1",
      "2"
    ]
  },
  "features": [
    {
      "name": "GLCM_Pan",
      "description": "GLCM mean texture (Pan band)",
      "dataType": "NUMERIC",
      "example": 120.362774,
      "nullable": false
    },
    {
      "name": "Mean_G",
      "description": "Mean green value",
      "dataType": "NUMERIC",
      "example": 205.5,
      "nullable": false
    },
    {
      "name": "Mean_R",
      "description": "Mean red value",
      "dataType": "NUMERIC",
      "example": 119.395349,
      "nullable": false
    },
    {
      "name": "Mean_NIR",
      "description": "Mean NIR value",
      "dataType": "NUMERIC",
      "example": 416.581395,
      "nullable": false
    },
    {
      "name": "SD_Plan",
      "description": "Standard deviation (Pan band)",
      "dataType": "NUMERIC",
      "example": 20.676318,
      "nullable": false
    }
  ]
}