{
  "title": "On Time Graduation Classification",
  "description": "This is a GPA dataset gathered from a university in Indonesia. However, this dataset is imbalanced",
  "modelCategory": "binary_classification",
  "schemaVersion": 1.0,
  "inputDataFormat": "CSV",
  "encoding": "utf-8",
  "id": {
    "name": "id",
    "description": "unique identifier"
  },
  "target": {
    "name": "tepat",
    "description": "binary class (Tidak or Ya)",
    "classes": [
      "Ya",
      "Tidak"
    ]
  },
  "features": [
    {
      "name": "ip1",
      "description": "GPA from semester 1",
      "dataType": "NUMERIC",
      "example": 2.3,
      "nullable": false
    },
    {
      "name": "ip2",
      "description": "GPA from semester 2",
      "dataType": "NUMERIC",
      "example": 1.97,
      "nullable": false
    },
    {
      "name": "ip3",
      "description": "GPA from semester 3",
      "dataType": "NUMERIC",
      "example": 1.8,
      "nullable": false
    },
    {
      "name": "ip4",
      "description": "GPA from semester 4",
      "dataType": "NUMERIC",
      "example": 1.56,
      "nullable": false
    }
  ]
}